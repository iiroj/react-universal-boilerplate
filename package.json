{
  "name": "react-universal-boilerplate",
  "version": "1.0.0",
  "description": "An example React/Redux application with SSR and code-splitting.",
  "license": "MIT",
  "author": "Iiro JÃ¤ppinen <iiro@jappinen.fi> (https://iiro.fi)",
  "homepage": "https://gitlab.com/iiroj/react-universal-boilerplate",
  "repository": "gitlab:iiroj/react-universal-boilerplate",
  "bugs": "https://gitlab.com/iiroj/react-universal-boilerplate/issues",
  "private": true,
  "scripts": {
    "dev": "BABEL_ENV=server_development nodemon -e js,jsx -i src/client -i build --exec babel-node -- src/server/index.js -x '.js,.jsx'",
    "lint": "eslint src",
    "build:client": "NODE_ENV=production BABEL_ENV=client_production webpack --config ./config/webpack.config.js",
    "build:server": "BABEL_ENV=server_production babel src -x '.js,.jsx' -d dist --copy-files",
    "prebuild": "npm run clean",
    "build": "concurrently npm:build:*",
    "stats": "webpack-bundle-analyzer build/stats.json",
    "clean": "rimraf dist build",
    "start": "NODE_ENV=production node dist/server/index.js",
    "version": "standard-version",
    "prerelease": "npm run lint && npm run build",
    "release": "git push --follow-tags origin master"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "babel-plugin-universal-import": "^3.0.2",
    "body-parser": "^1.18.3",
    "common-tags": "^1.8.0",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.5.7",
    "cors": "^2.8.4",
    "css-wipe": "^4.3.0",
    "emotion": "^9.2.8",
    "emotion-server": "^9.2.8",
    "express": "^4.16.3",
    "fontfaceobserver": "^2.0.13",
    "helmet": "^3.13.0",
    "history": "^4.7.2",
    "htmlescape": "^1.1.1",
    "immer": "^1.6.0",
    "memory-cache": "^0.2.0",
    "morgan": "^1.9.0",
    "node-object-hash": "^1.4.1",
    "raf": "^3.4.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.3.6",
    "react-redux": "^5.0.7",
    "react-universal-component": "^3.0.1",
    "redux": "^4.0.0",
    "redux-first-router": "0.0.16-next",
    "redux-first-router-link": "^1.4.2",
    "redux-saga": "^0.16.0",
    "webpack-flush-chunks": "^2.0.1",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
