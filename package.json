{
  "name": "react-universal-boilerplate",
  "version": "5.2.15",
  "description": "An example React/Redux application with SSR and code-splitting.",
  "license": "MIT",
  "author": "Iiro JÃ¤ppinen <iiro@jappinen.fi> (https://iiro.fi)",
  "homepage": "https://github.com/iiroj/react-universal-boilerplate",
  "repository": "github:iiroj/react-universal-boilerplate",
  "bugs": "https://github.com/iiroj/react-universal-boilerplate/issues",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development BABEL_ENV=node_development nodemon -w src/server -x babel-node -- src/server/index.js",
    "lint": "eslint .",
    "build:client": "NODE_ENV=production BABEL_ENV=node_production webpack",
    "build:server": "NODE_ENV=production BABEL_ENV=node_production babel src -d dist",
    "prebuild": "npm run clean",
    "build": "concurrently npm:build:*",
    "stats": "webpack-bundle-analyzer build/stats.json",
    "clean": "rimraf dist build",
    "start": "NODE_ENV=production node dist/server/index.js",
    "version": "standard-version --no-verify"
  },
  "lint-staged": {
    "*.js": "npm run lint"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "standard-version": {
    "scripts": {
      "prerelease": "npm run lint && npm run build"
    }
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@loadable/component": "^5.12.0",
    "@loadable/server": "^5.12.0",
    "body-parser": "^1.19.0",
    "common-tags": "^1.8.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "core-js": "^3.6.4",
    "cors": "^2.8.5",
    "css-wipe": "^4.3.0",
    "express": "^4.17.1",
    "fontfaceobserver": "^2.1.0",
    "helmet": "^3.21.3",
    "history": "^4.10.1",
    "memory-cache": "^0.2.0",
    "morgan": "^1.9.1",
    "node-object-hash": "^2.0.0",
    "raf": "^3.4.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet-async": "^1.0.4",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-style-reset": "^1.3.1",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/runtime": "^7.8.7",
    "@commitlint/config-conventional": "^8.3.4",
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@loadable/babel-plugin": "^5.12.0",
    "@loadable/webpack-plugin": "^5.12.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-transform-export-default-name": "^2.0.4",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "commitlint": "^8.3.5",
    "concurrently": "^5.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react-app": "^6.2.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "rimraf": "^3.0.2",
    "standard-version": "^7.1.0",
    "stats-webpack-plugin": "^0.7.0",
    "terser-webpack-plugin": "^2.3.5",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  }
}
