{
  "name": "react-universal-boilerplate",
  "version": "0.3.0",
  "description": "An example React/Redux application with SSR and code-splitting.",
  "license": "MIT",
  "author": "Iiro JÃ¤ppinen <iiro@jappinen.fi> (https://iiro.fi)",
  "homepage": "https://gitlab.com/iiroj/react-universal-boilerplate",
  "repository": "gitlab:iiroj/react-universal-boilerplate",
  "bugs": "https://gitlab.com/iiroj/react-universal-boilerplate/issues",
  "private": true,
  "scripts": {
    "dev": "BABEL_ENV=server_development nodemon -e js,jsx -i src/client -i build --exec babel-node -- src/server/index.js -x '.js,.jsx'",
    "lint": "eslint src",
    "build:client": "NODE_ENV=production BABEL_ENV=client_production webpack --config ./config/webpack.config.js",
    "build:server": "BABEL_ENV=server_production babel src -x '.js,.jsx' -d dist --copy-files",
    "prebuild": "npm run clean",
    "build": "concurrently npm:build:*",
    "stats": "webpack-bundle-analyzer build/stats.json",
    "clean": "rimraf dist build",
    "start": "NODE_ENV=production node dist/server/index.js"
  },
  "dependencies": {
