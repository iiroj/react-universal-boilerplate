{
  "name": "react-universal-boilerplate",
  "version": "5.2.11",
  "description": "An example React/Redux application with SSR and code-splitting.",
  "license": "MIT",
  "author": "Iiro JÃ¤ppinen <iiro@jappinen.fi> (https://iiro.fi)",
  "homepage": "https://github.com/iiroj/react-universal-boilerplate",
  "repository": "github:iiroj/react-universal-boilerplate",
  "bugs": "https://github.com/iiroj/react-universal-boilerplate/issues",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development BABEL_ENV=node_development nodemon -w src/server -x babel-node -- src/server/index.js",
    "lint": "eslint .",
    "build:client": "NODE_ENV=production BABEL_ENV=node_production webpack",
    "build:server": "NODE_ENV=production BABEL_ENV=node_production babel src -d dist",
    "prebuild": "npm run clean",
    "build": "concurrently npm:build:*",
    "stats": "webpack-bundle-analyzer build/stats.json",
    "clean": "rimraf dist build",
    "start": "NODE_ENV=production node dist/server/index.js",
    "version": "standard-version --no-verify"
  },
  "lint-staged": {
    "*.js": "npm run lint"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "standard-version": {
    "scripts": {
      "prerelease": "npm run lint && npm run build"
    }
  },
  "dependencies": {
    "@emotion/core": "^10.0.14",
    "@loadable/component": "^5.10.1",
    "@loadable/server": "^5.9.0",
    "body-parser": "^1.19.0",
    "common-tags": "^1.8.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "core-js": "^3.1.4",
    "cors": "^2.8.5",
    "css-wipe": "^4.3.0",
    "express": "^4.17.1",
    "fontfaceobserver": "^2.1.0",
    "helmet": "^3.18.0",
    "history": "^4.9.0",
    "memory-cache": "^0.2.0",
    "morgan": "^1.9.1",
    "node-object-hash": "^1.4.2",
    "raf": "^3.4.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet-async": "^1.0.2",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-style-reset": "^1.2.9",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/node": "^7.5.0",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.5.4",
    "@commitlint/config-conventional": "^8.0.0",
    "@emotion/babel-preset-css-prop": "^10.0.14",
    "@loadable/babel-plugin": "^5.10.0",
    "@loadable/webpack-plugin": "^5.7.1",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-transform-export-default-name": "^2.0.4",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "commitlint": "^8.0.0",
    "concurrently": "^4.1.1",
    "dotenv": "^8.0.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react-app": "^5.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "husky": "^3.0.0",
    "lint-staged": "^9.2.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "rimraf": "^2.6.3",
    "standard-version": "^6.0.1",
    "stats-webpack-plugin": "^0.7.0",
    "terser-webpack-plugin": "^1.3.0",
    "typescript": "^3.5.3",
    "webpack": "^4.35.3",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0"
  }
}
