    "raf": "^3.4.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-emotion": "^9.2.6",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.3.3",
    "react-redux": "^5.0.7",
    "react-universal-component": "^2.9.0",
    "redux": "^4.0.0",
