{
  "name": "react-universal-boilerplate",
  "version": "3.2.1",
  "description": "An example TypeScript React/Redux application with SSR and code-splitting.",
  "license": "MIT",
  "author": "Iiro JÃ¤ppinen <iiro@jappinen.fi> (https://iiro.fi)",
  "homepage": "https://gitlab.com/iiroj/react-universal-boilerplate",
  "repository": "gitlab:iiroj/react-universal-boilerplate",
  "bugs": "https://gitlab.com/iiroj/react-universal-boilerplate/issues",
  "private": true,
  "scripts": {
    "dev": "BABEL_ENV=server_development nodemon -w src/server -x babel-node -- src/dev.js",
    "lint": "tslint --project tsconfig.json",
    "build:client": "NODE_ENV=production BABEL_ENV=server_production webpack --config ./config/webpack.config.js",
    "build:server": "NODE_ENV=production BABEL_ENV=server_production babel src -d dist --extensions '.ts,.tsx'",
    "prebuild": "npm run clean",
    "build": "concurrently npm:build:*",
    "stats": "webpack-bundle-analyzer build/stats.json",
    "clean": "rimraf dist build",
    "start": "NODE_ENV=production node dist/server/index.js",
    "version": "standard-version",
    "prerelease": "npm run lint && npm run build",
    "release": "git push --follow-tags origin master"
  },
